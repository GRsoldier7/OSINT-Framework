{
  "name": "ultimate-osint-framework",
  "version": "1.0.0",
  "description": "A comprehensive, browser-based Open Source Intelligence (OSINT) framework",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,scss,md}\"",
    "docs": "jsdoc -c jsdoc.json",
    "analyze": "vite build --mode analyze"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GRsoldier7/OSINT-Framework.git"
  },
  "keywords": [
    "osint",
    "intelligence",
    "investigation",
    "research",
    "security",
    "framework"
  ],
  "author": "OSINT Framework Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GRsoldier7/OSINT-Framework/issues"
  },
  "homepage": "https://github.com/GRsoldier7/OSINT-Framework#readme",
  "dependencies": {
    "axios": "^1.6.2",
    "d3": "^7.8.5",
    "date-fns": "^2.30.0",
    "dexie": "^3.2.4",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "idb": "^7.1.1",
    "leaflet": "^1.9.4",
    "lodash": "^4.17.21",
    "pinia": "^2.1.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0",
    "vis-network": "^9.1.6",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@testing-library/react": "^14.1.2",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.53.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdoc": "^4.0.2",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "tailwindcss": "^3.3.5",
    "vite": "^5.0.0",
    "vite-plugin-pwa": "^0.17.0",
    "vitest": "^0.34.6"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
